<div>
  <h1>My Makan</h1><br>
  <ul style="list-style-type:square">
    <% @users_meals.each do |user_meal| %>
        <li>
          Poll end: <%= user_meal.meal.endtime.localtime.strftime('%e %b %Y %I:%M %P') %>
          Meal ID: <%= link_to user_meal.meal.vanity_id, meal_path(user_meal.meal.vanity_id)%> |
        </li>
    <% end %>
  </ul>
</div>
